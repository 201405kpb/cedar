<script>

  var chart = new Cedar({"type":"time"});

  var datasets = [
    {
      "url": "https://services.arcgis.com/bkrWlSKcjUDFDtgw/arcgis/rest/services/It's_a_Tornado_Map/FeatureServer/0",
      "query": {
        "orderByFields": "Date"
      }
    }
  ];

  var series = [
    {
      "category": { "field": "Date", "label": "Date" },
      "value": { "field":"Injuries","label":"Injuries" }
    }
  ];

  chart.tooltip = {
    "title": "{Year}",
    "content": "{Injuries} injuries<br/>{Fatalities} fatalities"
  };

  chart.datasets = datasets;
  chart.series = series;
  //chart.dataset.mappings.sort = "DATE";
  chart.show({
    elementId: "#chart",
    autolabels: true
  });


</script>
